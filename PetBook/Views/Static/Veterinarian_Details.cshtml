@{
    Layout = null;
}
    <h2>Veterinarian Details Test</h2>
        <div class="form-horizontal">
            <ul id="validationSummary"></ul>
            <div class="form-group">
                <label for="Name" class = "control-label col-md-2">
                Name
                </label>
                <div class="col-md-10">
                    {{data.Name}}
                </div>
            </div>
    
            <div class="form-group">
                <label for="Address" class = "control-label col-md-2">
                Address
                </label>
                <div class="col-md-10">
                    {{data.Address}}
                </div>
            </div>
    
            <div class="form-group">
                <label for="Age" class = "control-label col-md-2">
                Age
                </label>
                <div class="col-md-10">
                    {{data.Age}}
                </div>
            </div>
    
            <div class="form-group">
                <label for="Gender" class = "control-label col-md-2">
                Gender
                </label>
                <div class="col-md-10">
                    {{data.Gender}}
                </div>
            </div>
    
            <div class="form-group">
                <label for="Location" class = "control-label col-md-2">
                Location
                </label>
                <div class="col-md-10">
                    {{data.Location}}
                </div>
            </div>
    
            <div class="form-group">
                <label for="Email" class = "control-label col-md-2">
                Email
                </label>
                <div class="col-md-10">
                    {{data.Email}}
                </div>
            </div>
    
            <div class="form-group">
                <label for="PhoneNumber" class = "control-label col-md-2">
                PhoneNumber
                </label>
                <div class="col-md-10">
                    {{data.PhoneNumber}}
                </div>
            </div>
    
            <div class="form-group">
                <label for="Username" class = "control-label col-md-2">
                Username
                </label>
                <div class="col-md-10">
                    {{data.Username}}
                </div>
            </div>
    
            <div class="form-group">
                <label for="Password" class = "control-label col-md-2">
                Password
                </label>
                <div class="col-md-10">
                    {{data.Password}}
                </div>
            </div>
    
            <div class="form-group">
                <label for="NameOfBusiness" class = "control-label col-md-2">
                NameOfBusiness
                </label>
                <div class="col-md-10">
                    {{data.NameOfBusiness}}
                </div>
            </div>
    
            <div class="form-group">
                <label for="Customers" class = "control-label col-md-2">
                 Customers
                </label>
                <div class="col-md-10">
                    <a ng-href="#Customer?Veterinarian={{data.Id }}" >
                    {{data.Customers_Count}}
                    </a>
                </div>
            </div>
    
            <div class="form-group">
                <label for="Appointments" class = "control-label col-md-2">
                 Appointments
                </label>
                <div class="col-md-10">
                    <a ng-href="#Appointment?Veterinarian={{data.Id }}" >
                    {{data.Appointments_Count}}
                    </a>
                </div>
            </div>
    
           
        </div>
    
    <div>
        <a href="#/Veterinarian">Back to List</a>
    </div>
