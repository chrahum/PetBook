@{
    Layout = null;
}
    <h2>Appointment</h2>
        <div class="form-horizontal">
            <ul id="validationSummary"></ul>
            Are you sure that you want to delete {{Appointment}}?
            <div class="form-group">
                <label for="Date" class = "control-label col-md-2">
                Date
                </label>
                <div class="col-md-10">
                    {{data.Date}}
                </div>
            </div>
    
            <div class="form-group">
                <label for="IsFirstVisit" class = "control-label col-md-2">
                IsFirstVisit
                </label>
                <div class="col-md-10">
                    {{data.IsFirstVisit}}
                </div>
            </div>
    
            <div class="form-group">
                <label for="Veterinarian" class = "control-label col-md-2">
                 Veterinarian
                </label>
                <div class="col-md-10">
                    {{data.Veterinarian_Name}}
                </div>
            </div>
    
            <div class="form-group">
                <label for="Customer" class = "control-label col-md-2">
                 Customer
                </label>
                <div class="col-md-10">
                    {{data.Customer_Name}}
                </div>
            </div>
    
            <div class="form-group">
                <div class="col-md-offset-2 col-md-10">
                    <input type="submit" value="Save" class="btn btn-default" ng-click="save()" />
                </div>
            </div>
        </div>
    
    <div>
        <a href="#/Appointment">Back to List</a>
    </div>
