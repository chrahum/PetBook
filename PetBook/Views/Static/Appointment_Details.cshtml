@{
    Layout = null;
}
    <h2>Appointment Details Test</h2>
        <div class="form-horizontal">
            <ul id="validationSummary"></ul>
            <div class="form-group">
                <label for="Date" class = "control-label col-md-2">
                Date
                </label>
                <div class="col-md-10">
                    {{data.Date}}
                </div>
            </div>
    
            <div class="form-group">
                <label for="IsFirstVisit" class = "control-label col-md-2">
                IsFirstVisit
                </label>
                <div class="col-md-10">
                    {{data.IsFirstVisit}}
                </div>
            </div>
    
            <div class="form-group">
                <label for="Veterinarian" class = "control-label col-md-2">
                 Veterinarian
                </label>
                <div class="col-md-10">
                    <a ng-href="#Veterinarian/{{Veterinarian }}" >
                    {{data.Veterinarian_Name}}
                    </a>
                </div>
            </div>
    
            <div class="form-group">
                <label for="Customer" class = "control-label col-md-2">
                 Customer
                </label>
                <div class="col-md-10">
                    <a ng-href="#Customer/{{Customer }}" >
                    {{data.Customer_Name}}
                    </a>
                </div>
            </div>
    
           
        </div>
    
    <div>
        <a href="#/Appointment">Back to List</a>
    </div>
